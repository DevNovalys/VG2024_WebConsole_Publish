{
  "VGWebConfiguration": {
    "ConnectionTimeOut": 600,
    "ExcludePageUrls": [
      "/Identity/Account/Login",
      "/Identity/Account/Logout",
      "/Identity/Account/ChangePassword",
      "/Identity/Account/forgotpassword",
      "/home/setculture",
      "/home/error",
      "/error",
      "/VGConfigure/Index",
      "/VGConfigure/SqlServer/GetDatabases",
      "/VGConfigure/SqlServer/Configure",
      "/progressHub",
      "/vgconfigure/progressHub",
      "/VGConfigure/SqlServer/NeedMigrate",
      "VGConfigure/SqlServer/TestConnection",
      "VGConfigure/Oracle/TestConnection",
      "VGConfigure/.*"
    ],
    "ExcludeExtensions": [
      ".css",
      ".png",
      ".js",
      ".gif",
      ".jpg",
      ".Gif",
      ".jpeg",
      ".axd",
      ".dxr",
      ".ico",
      ".woff",
      ".woff2",
      ".ttf",
      ".map"
    ],
    "LoginUrl": "/Identity/Account/Login",
    "TraceLevel": "Off",
    "UIConfig": {
      "Theme": 0,
      "MenuOnTop": false,
      "FixedRibbon": false,
      "FixedNavigation": true,
      "FixedHeader": true
    },
    "VGMode": "Standard"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "SmartSettings": {
    "Version": "4.3.2",
    "App": "Visual Guard Web Console",
    "AppName": "Visual Guard Web Console",
    "AppFlavor": ".NET 6.0",
    "AppFlavorSubscript": ".NET6.0",
    "StoreLocally": false,
    "Theme": {
      "ThemeVersion": "4.5.1",
      "IconPrefix": "fal",
      "Logo": "logo.png",
      "User": "Novalys",
      "Email": "support@novalys.net",
      "Avatar": ""
    },
    "Features": {
      "AppSidebar": true,
      "AppHeader": true,
      "AppLayoutShortcut": true,
      "AppFooter": true,
      "ShortcutMenu": true,
      "ChatInterface": true,
      "LayoutSettings": true
    }
  },
  "AllowedHosts": "*"
}